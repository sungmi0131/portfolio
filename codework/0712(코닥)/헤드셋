<script type="text/javascript">
function alertPopMsgLogin(c_msg,c_fn) {
    var $body = $('body');
    var popId = 'alertPop';
    var popHtml = "";
    if ($('#'+ popId).length) {
        $('#'+ popId).remove();
    }
    $body.append('<div id="'+popId+'"/>').find('#'+ popId).html('<div class="alert" />');
    popHtml += '<div class="alertInner"> ';
	popHtml += '    <div class="text"> ';
	popHtml += '        <p class="slg">'+c_msg+'</p>  ';
	popHtml += '    </div> ';
	popHtml += '    <div class="btn-wrap"> ';
		popHtml += '<a href="javascript:void(0);" onclick="pageMove(\'login\');" class="btn btn-type-s-4 btn-color-type9">로그인</a>';
	popHtml += '<a href="../member/register-intro.do" class="btn btn-type-s-4 btn-color-type1">회원가입</a>';
	popHtml += '    </div> ';
	popHtml += '</div> ';
    $('#alertPop').find('.alert').html(popHtml);		
}
</script>

