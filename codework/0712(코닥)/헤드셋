        $('.scroll-section').each(function (index) {
            var bottom = $(this).offset().top + $(this).height();
            if ($(window).scrollTop() >= $(this).offset().top && $(window).scrollTop() <= bottom) {
                $('.section-navi ul li').removeClass('active');
                $('.section-navi ul li').eq(index).addClass('active');
            }
        });
        
    });

