    textarea.each(function () {
        $(this).outerHeight();
        if ($(this).outerHeight() > 20) {
            var thisHeight = $(this).outerHeight();
            // console.log(thisHeight)
            $(this).addClass('hidden');
            var btnMoreCmt = $(this).parents('.text-area');
            // btnMoreCmt.show();
            btnMoreCmt.on("click", function (e) {
                e.preventDefault();
                var $this = $(this)
                 $this.parents('.review-content').toggleClass('active');
                 
                if( ! $this.parents('.review-content').hasClass('active')){
                    $this.find('.review-text').css('height' , '20px')
                    $this.find('.review-text').removeClass('review-opened')
                    $this.next('.edit-btn-wrap').css('display', 'none')
                }else{
                    // $this.find('.review-text').css('height', thisHeight +'px')
                    $this.find('.review-text').css('height', 'auto')
                    $this.find('.review-text').addClass('review-opened')
                    $this.next('.edit-btn-wrap').css('display', 'block')
                 }
                
            });
