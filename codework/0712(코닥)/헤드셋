    $detailReivewImg.each(function (index, item) {
        if ($(this).height() / $(this).width() < 0.567) {
            $(this).addClass('hor-img').removeClass('ver-img');
        } else {
            $(this).addClass('ver-img').removeClass('hor-img');
        }
    });

    // review tab event
