Vue.component('app-scroll-top', {
    template: `
            <div class="scroll-top-wrap" v-show="scrollpx > 500" >
                <div class="scroll-top-inner">    
                    <div class="move-btn move-to-top" @click="toTop">
                        <span class="scroll-top-btn"></span>
                    </div>
                </div>
            </div>
        `,

        data: () => {
            return {
                scrollTop: 0,
                scrollpx: 0,
            }
        },
        methods: {
            handleScroll() {
                this.scrollpx = window.scrollY;
                if(this.scrollpx < 500){
                    // $('.move-btn').addClass('less')
                }
            },
        
            toTop: function () {
                $("html, body").animate({ scrollTop: 0 }, 500);
                
            },
        },
        created () {
            window.addEventListener('scroll', this.handleScroll);
        },
        destroyed() {
            window.removeEventListener('scroll', this.handleScroll);
        },
});




// 검색 팝업
      
Vue.component('app-search-pop', {
    name : "wrap",
    data: () => {
        return {
            sandState  : false,
            sandData   : null,
            searchMode : false,
            search: "",
            itemsdata: null,
            total: null,                   // 제품 전체 갯수                    || 신상품
            itemHitData: null,
        }
    },
    created: function () {
    	this.itemSearchData = _searchKeyword_ob ;
    	
        axios.get("../products/listKeywd.json").then((res) => {
        	this.itemHitData = res.data.data;
        }).catch(err => {
            console.log(err);
        });

    	
       /*  axios.get("../res/data/temp2.json").then((res) => {
            this.itemsdata = res.data.products;
            this.total = res.data.products.length;
        }).catch(err => {
            console.log(err);
        }); */
    },
    mounted: function () {
        this.focusevent()

        
    },
    computed: {
    	itemSearchParseData() {
    		return _searchKeyword_ob ;
    	}  
    },
    methods: {
        reset(){
            this.sandState = false;
            this.search = "";
            this.filterReset();
            this.sortDir = '신상품'
        },
        send(){
            if (this.search == "") {
            	alertPopMsg('검색어를 입력하세요.');
            	return 
            }
            this.searchMode = false;
            this.sandState  = true;
            window.location.href="../sub/search.do?searchKeyword="+encodeURIComponent( this.search)
        },
        searchfind(e){
            e.stopPropagation();
            this.search = e.target.closest('li').innerText;
            this.send();
            // this.search = e.target.value;
        },
        searchfind2(e) {
            e.stopPropagation();
            var text = e.target.closest('a').innerText;
            text = text.replace(/#/gi, "");
            this.search = text;
            this.send();
            return this.search;
        },
        hisDelAll(e) {
        	delSearchKeyword('__all__');
        	// console.log(_text);
        	e.stopPropagation();
        	// $(_ob).parent().parent().remove();
            this.itemSearchData = _searchKeyword_ob ;
            this.$forceUpdate();
        },        
        hisDel(_text, _ob,e) {
        	delSearchKeyword(_text);
        	// console.log(_text);
        	e.stopPropagation();
        	// $(_ob).parent().parent().remove();
            this.itemSearchData = _searchKeyword_ob ;
            this.$forceUpdate();
        },        
        changeValue(e) {
            // console.log(e.target.value);
        	this.search = e.target.value;
        	this.sandState = true;
            setAutoKeyWdItem(e.target.value , this);
        },
        heig(val) {
            // console.log(search)
            var search = this.search
            var regex = new RegExp(search, "g");
            return String(val).replace(regex, "<span>" + search + "</span>");
        },
        focusevent() {
            // console.log(this.$refs.searchA)
            this.$nextTick(function () {
              this.$refs.searchA.focus();
          })
        }
    },
    template: `
        <div>
            <div class="search-top">
                <div class="all-search">
                    
                    <div class="input-box">
                        <input type="text" :value="search" id="brandSearchInput" placeholder="검색어를 입력"
                            @input="changeValue" @focus="searchMode = true" @keyup.enter="send" ref="searchA" autofocus/>
                        <ul class="search-result custom-scroll"
                            v-if="search != '' && itemsdata && searchMode">
                            <li v-for="(item, index) in itemsdata"  @click="searchfind">
                                <a
                                    v-html="heig(item.brd_name)"
                                    :href="'../sub/search.do?searchKeyword=' + item.brd_name"
                                ></a>
                            </li>
                        </ul>
                    </div>
                    <template v-if="sandState == false">
                        <button class="search-find" @click=send>
                            <img src="../res/img/common/search_icon4.png" alt="검색">
                        </button>
                    </template>
                    <template v-else>
                        <button class="search-text-del" @click="reset">
                            <img src="../res/img/sub/brandClose.png" alt="삭제">
                        </button>
                    </template>

                </div>
            </div>
            <div class="search-word" v-if="sandState == false">
                <div class="his-word">
                    <h3>최근검색어 <button class="del" @click="hisDelAll($event)">전체 삭제</button></h3>
                    <div class="hash">
	                	<template v-for="(text, index) in itemSearchData" v-if="index < 5" >
	                    <span>
	                		<a :href="'../sub/search.do?searchKeyword='+ encodeURIComponent(text)">{{text}}</a> 
	                		<button class="del" @click="hisDel(text, this, $event)"><img src="../res/img/common/close-icon7.png" alt="삭제"></button>
	                    </span>
	                    </template>
	                    <template v-if="!itemSearchData || itemSearchData.length == 0">
	                    <span class="search-empty">최근 검색어가 없습니다.</span>
	                    </template>
                    </div>
                </div>
                <div class="hot-word">
                    <h3>인기검색어</h3>
                    <div class="hash">
	                    <template v-for="(text, index) in itemHitData"  v-if="index < 5" >
	                    <span>
	                		<a :href="'../sub/search.do?searchKeyword='+ text">{{text}}</a> 
	                    </span>
	                    </template>                    
                    </div>
                </div>
            </div>
        </div>
            `
});

</script>


<script>

var search;
var _main_nav_index ;
if($('#header').length){
	search = new Vue({
	    el: '#header',
	    data: () => {
	        return {
	            serachPopShow: false,
	        }
	    },
	    created: function () {

			$('.main-nav > div > div').each(function (index) {
		        var _url = $(this).find('a').attr('href');
		        if(_url){
		        	if(window.location.href.indexOf( _url.replace('..','')) >= 0){
		        		$(this).addClass('active');
		        		_main_nav_index = index;
						return false;        		
		        	}
		        }
		    })
	    	
	    },
	    methods: {
	        searchPopUpOpen() {
				if ($('html, body').hasClass('no-scroll')) {
					$('html, body').removeClass('no-scroll');
				} else {
					$('html, body').addClass('no-scroll');
				}

				
	            this.serachPopShow = !this.serachPopShow;
	            this.$refs.focusheader.focusevent()
	        },
	    },
	})
}
function showSearch(){
	search.serachPopShow = true;
	this.$refs.focusheader.focusevent()
	if ($('html, body').hasClass('no-scroll')) {
		$('html, body').removeClass('no-scroll');
	} else {
		$('html, body').addClass('no-scroll');
	}
}


