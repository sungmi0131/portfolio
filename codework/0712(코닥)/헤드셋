function goBack(){
	if(document.referrer){
		window.history.back();	
	}else{
		window.location.href ="/shopMo/main/main.do";
	}
	
}		

function isObject(val) {
	return !(typeof val === 'undefined'); 
}


var keyword_find_old = '' ;
var _key_time ;

function setAutoKeyWdItem(_text, _this) {

	_this.searchMode = true;
	if( new Date()- _key_time < 1000){
		return;
	}
	var keyword = _text;
	var keyword_find = '';
	const txtChk = /[0-9|a-z|A-Z|가-힣]/;
	if(txtChk.test(keyword ) ){
		if(keyword.length > 1){
			var keyword_last = keyword.substr(keyword.length-1);
			if(txtChk.test(keyword_last )){
				keyword_find = keyword;
			}else{
				keyword_find = keyword.substr(0,keyword.length-1);
			}
		}else {
			keyword_find = keyword;
		}
	} else {
		
		
	}


	if(keyword_find_old == keyword_find){
		return;
	} 

	if(keyword_find == '') {
		_this.itemsdata = [];
		keyword_find_old=''
	}
	
	console.log(keyword_find);
	if( keyword_find  == ''){

	}else{
		_key_time = new Date();
		keyword_find_old = keyword_find ;
		// /data/getBrnList.do
		var _url = '../data/getAutoKeyList.json?searchKeyword='+keyword_find;
		// url = "../products/prodList.json?pageIndex=1&pageUnit=32&searchCondition=name&searchKeyword="+keyword_find ;
		axios.get(_url ).then((res) => {
			 _this.itemsdata = res.data.data;
        }).catch(err => {
            console.log(err);
        });
	}


}

function setCookie(name, value, day) {
	    var date = new Date();
	    date.setTime(date.getTime() + day * 60 * 60 * 24 * 1000);
	    document.cookie = name + '=' + value + ';expires=' + date.toUTCString() + ';path=/';
	};
	
	// 쿠키 가져오기 함수
	function getCookie(cName) {
		cName = cName + '=';
		var cookieData = document.cookie;
		var start = cookieData.indexOf(cName);
		var cValue = '';
		if(start != -1){
			start += cName.length;
			var end = cookieData.indexOf(';', start);
			if(end == -1)end = cookieData.length;
				cValue = cookieData.substring(start, end);
		}
		return unescape(cValue);
	}
	
	$(document).on('click', 'a._none_href' , function(e){e.preventDefault(); } )
	
	function makePaging(paginationInfo){
		$('#total_cnt').html(paginationInfo.totalRecordCount);
		var lastPageNoOnPageList = paginationInfo.lastPageNoOnPageList;
		var firstPageNoOnPageList = paginationInfo.firstPageNoOnPageList;
		var currentPageNo = paginationInfo.currentPageNo;
		var lastPage = ((paginationInfo.totalRecordCount -1) / paginationInfo.recordCountPerPage) + 1;
		
		var paginghtml = '';
		
		if(firstPageNoOnPageList == 1){
			paginghtml += ' <button class="paging-prev" onclick="pageGo(\'1\');" ><img src="../res/img/common/prev_icon.png" alt="이전"> </button> ';
		}else{
			paginghtml += ' <button class="paging-prev" onclick="pageGo(\''+(firstPageNoOnPageList - 1)+'\');" ><img src="../res/img/common/prev_icon.png" alt="이전"> </button> ';	
		}
		for (var i = firstPageNoOnPageList; i <= lastPageNoOnPageList; i++) {
			if(currentPageNo == i){
				paginghtml += '	<button class="paging-num active">'+i+'</button> ';
			}else{
				paginghtml += '	<button class="paging-num" onclick="pageGo(\''+i+'\');" >'+i+'</button> ';
			}
		}
		
		if(lastPageNoOnPageList + 1 > lastPage){
			paginghtml += ' <button class="paging-next" onclick="pageGo(\''+lastPageNoOnPageList+'\');" > <img src="../res/img/common/next_icon.png" alt="이전"> </button> ';	
		}else{
			paginghtml += ' <button class="paging-next" onclick="pageGo(\''+(lastPageNoOnPageList + 1)+'\');" > <img src="../res/img/common/next_icon.png" alt="이전"> </button> ';
		}
		$('.paging').html(paginghtml);
		$('.paging').show();
	}
	
	function alertPopMsg(c_msg,c_fn) {
	    var $body = $('body');
	    var popId = 'alertPop';
	    var popHtml = "";
	    if ($('#'+ popId).length) {
	        $('#'+ popId).remove();
	    }
	    $body.append('<div id="'+popId+'"/>').find('#'+ popId).html('<div class="alert" />');
	    popHtml += '<div class="alertInner"> ';
    	popHtml += '    <div class="text"> ';
   		popHtml += '        <p class="slg">'+c_msg+'</p>  ';
		popHtml += '    </div> ';
		popHtml += '    <div class="btn-wrap"> ';
		if(c_fn){
			popHtml += '        <button type="button" onclick="' + c_fn + '" class="btn btn-type-s-4 btn-single btn-color-type1">확인</button> ';
		}else{
			popHtml += '        <button type="button" onclick="alertClose()" class="btn btn-type-s-4 btn-single btn-color-type1">확인</button> ';
		}
		popHtml += '    </div> ';
		popHtml += '</div> ';
	    $('#alertPop').find('.alert').html(popHtml);		
	}
	
	function alertPopSearchType(c_type,c_value) {
	    var $body = $('body');
	    var popId = 'alertPop';
	    var popHtml = "";
	    if ($('#'+ popId).length) {
	        $('#'+ popId).remove();
	    }
	    
	    $body.append('<div id="'+popId+'"/>').find('#'+ popId).html('<div class="alert" />');
	    if (c_type == 'id') {
	    	popHtml = `
		        <div class="alertInner">
		            <div class="text">
		            	<p class="slg">회원님의 정보와 일치하는<br>아이디입니다.</p>
		            	<p class="slg bold">`+c_value+`</p>
		            </div>
		            <div class="btn-wrap">
		           	 	<button onclick="window.location.href='../member/search-pw.do'" class="btn btn-type-s-4 btn-color-type9">비밀번호 찾기</button>
		           	 	<a href="javascript:void(0);" onclick="window.location.href='../member/login.do'" class="btn btn-type-s-4 btn-color-type1">로그인</a>
		            </div>
		        </div>
		     `
	    }else if (c_type == 'pw_phone') {
	    	popHtml = `
		        <div class="alertInner">
		            <div class="text">
		            	<p class="slg">회원님의 휴대폰 번호 </p>
		            	<p class="slg bold">`+c_value+`</p>
		            	<p class="slg">임시 비밀번호가 발송되었습니다.</p>
		            </div>
		            <div class="btn-wrap">
		           	 	<a href="javascript:void(0);" onclick="window.location.href='../member/login.do'" class="btn btn-type-s-4 btn-color-type1">로그인</a>
		            </div>
		        </div>
		     `
	    }else if (c_type == 'pw_email') {
	    	popHtml = `
	    		<div class="alertInner">
		            <div class="text">
		            	<p class="slg">회원님의 이메일주소 </p>
		            	<p class="slg bold">`+c_value+`</p>
		            	<p class="slg">임시 비밀번호가 발송되었습니다.</p>
		            </div>
		            <div class="btn-wrap">
		           	 	<a href="javascript:void(0);" onclick="window.location.href='../member/login.do'" class="btn btn-type-s-4 btn-color-type1">로그인</a>
		            </div>
		        </div>
		     `
	    }
	    
		
		$('#alertPop').find('.alert').html(popHtml);
		
	}
	function alertPopType(c_type) {
	    var $body = $('body');
	    var popId = 'alertPop';
	    var popHtml = "";
	    if ($('#'+ popId).length) {
	        $('#'+ popId).remove();
	    }
		// 2022-04-19 스크롤 방지 추가 시작
		if ($body.hasClass('no-scroll')) {
            $body.addClass('no-scroll2')
        } else {
            $body.addClass('no-scroll');
        }
        // 2022-04-19 스크롤 방지 추가 끝
	    $body.append('<div id="'+popId+'"/>').find('#'+ popId).html('<div class="alert" />');
	    
	    
	    
	    if (c_type == 'option') { // 옵션 선택안한 경우
	        popHtml = `
	                    <div class="alertInner">
	                        <div class="text">
	                            <p class="slg">상품 옵션을 선택해 주세요.</p>
	                        </div>
	                        <div class="btn-wrap">
	                            <button onclick="alertClose()" class="btn btn-type-s-4 btn-single btn-color-type1">확인</button>
	                        </div>
	                    </div>
	                  `
	    }else if (c_type == 'non_member') {  // 비회원인 경우
	    	popHtml = `
                <div class="alertInner">
                    <div class="text">
                    	<p class="slg">로그인 후 이용 가능합니다.</p>
                    </div>
                    <div class="btn-wrap">
                   	 	<button onclick="alertClose()" class="btn btn-type-s-4 btn-color-type9">취소</button>
                   	 	<a href="javascript:void(0);" onclick="pageMove('login');" class="btn btn-type-s-4 btn-color-type1">로그인</a>
                    </div>
                </div>
             `
	    } else if (c_type == 'login') {  //로그인 구매
	    	popHtml = `
			    	<div class="alertInner with-close-icon">
						<button onclick="alertClose()" class="pop-close-icon"><img src="../res/img/common/pop-close-icon.png"/></button>
						<div class="kakaoLoginWrap">`;
						var agent = navigator.userAgent.toLowerCase();
						if(window.location.href.indexOf('/products/products-detail.do') > -1 && agent.indexOf('chrome') < 0 && agent.search("android") < 0){
							popHtml = popHtml + `<button type="button" onclick="loginKakaoStart();" class="kakao-login-img btn btn-type-s-4""><img src="../res/img/common/kakaoLogin.png"/></button>` ;
						}
						popHtml = popHtml + `<a href="javascript:void(0);" class="btn btn-type-s-4 btn-color-type5" onclick="pageMove('login');" >로그인</a>
						</div>
					    <div class="text">
					        <p class="slg mt20">비회원으로 구매하시면 포인트,</p>
					        <p class="slg">20% 할인쿠폰 헤택을 받을 수 없습니다.</p>
					    </div>
					    <div class="btn-wrap">
							<button onclick="alertClose(exPurchase)" class="btn btn-type-s-4 c-pc2">[비회원 구매]</button>
					    </div>
						</div>
	           `
	    }else if (c_type == 'cart') {  //카트 이동
	    	popHtml = `
                <div class="alertInner">
                    <div class="text">
                        <p class="slg">선택하신 상품이 장바구니에 담겼습니다.</p>
                        <p class="slg bold">장바구니로 이동하시겠습니까?</p>
                    </div>
                    <div class="btn-wrap">
                        <button onclick="alertClose()" class="btn btn-type-s-4 btn-color-type9">계속 쇼핑</button>
                        <a href="../order/cart.do" class="btn btn-type-s-4 btn-color-type1">장바구니 이동</a>
                    </div>
                </div>
	                  `
	    } else if (c_type == 'amountMax') { //갯수 수량 초과
	    	/* 
		    	<p class="slg">해당 상품은 `+ max +`개 이상 주문하실 수 없습니다.</p>
	            <p class="slg">수량을 1회 최대 주문 가능 수량으로 변경해주세요</p> 
            */
	        popHtml = `
	                    <div class="alertInner">
	                        <div class="text">
	                            <p class="slg">해당 상품은 `+ max +`개이상</p>
	                            <p class="slg">주문하실 수 없습니다.</p>
	                            <p class="slg">1회에 최대로 주문가능한</p>
	                            <p class="slg">수량으로 변경해주세요.</p>
	                        </div>
	                        <div class="btn-wrap">
	                            <button onclick="alertClose()" class="btn btn-type-s-4 btn-single btn-color-type1">확인</button>
	                        </div>
	                    </div>
	                  `
	    }else if (c_type == 'inconsistency') { //구매수량과 동일 불일치
