<script type="text/javascript">
// 쿠키 가져오기 함수
function getCookie(cName) {
	cName = cName + '=';
	var cookieData = document.cookie;
	var start = cookieData.indexOf(cName);
	var cValue = '';
	if(start != -1){
		start += cName.length;
		var end = cookieData.indexOf(';', start);
		if(end == -1)end = cookieData.length;
			cValue = cookieData.substring(start, end);
	}
	return unescape(cValue);
}

function loginKakaoStartAuto(_param) {
	
	//객체 생성부분
	var xmlhttp;
	if (window.XMLHttpRequest) {
	    xmlhttp = new XMLHttpRequest();
	} 
	else {
	    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
	}
	 
	//Ajax구현부분
	xmlhttp.onreadystatechange = function() {
	    if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
	         //통신 성공시 구현부분
	         kakaoLoginHandlerAutoR(xmlhttp.responseText);
	    }
	}
	var _loginUrl = window.location.href;
	if(_loginUrl.indexOf('/member/register-intro.do') > 0 ){
		_loginUrl = 'https://www.frisbeekorea.com/shopMo/main/main.do';
	}
	
	if(!_param) _param ='snsAutoLoginType';
	
	xmlhttp.open("GET", "../member/kakaoStart.do?snsAddJoinType="+_param+"&goLogin2Url="+ encodeURIComponent(_loginUrl), true);
	xmlhttp.send();
	
} 


function kakaoLoginHandlerAutoR(_frisbeekey) {
	// 싱크 동의창에서 취소시 되돌아올 현재 로그인창 url
	
	var _reUrl = window.location.href; 
	if(window.location.search.length > 1 && window.location.search.indexOf('autoKakaoJoinCheck=Y') < 0 ){
		_reUrl = window.location.href + '&autoKakaoJoinCheck=Y';
	}
	
	let originUrlEncoded = encodeURIComponent('https://www.frisbeekorea.com/shopMo/member/snsLoginReturn.do?type=autoKakaoSnsLogin&goLogin2Url='+ encodeURIComponent(_reUrl));
	let state = JSON.stringify({originUrl: originUrlEncoded, frisbeekey: _frisbeekey})
	Kakao.Auth.authorize({
		redirectUri:  'http://www.frisbeekorea.com:9090/kkosync/v1/kakao_oauth' , // 싱크 에이전트 URL
		prompts: 'none',
		state: state
	})
}
var agent = navigator.userAgent.toLowerCase();
console.log('kakao_sns_chk_1');
if ( !login.isLogin && agent.indexOf('kakaotalk') >= 0 && (getCookie('autoKakaoSnsLogin') != 'N' || window.document.referrer == '') ){
	console.log('kakao_sns_chk_2');
	if(window.location.href.indexOf('/member/') < 0 && window.location.href.indexOf('/cSub/cEvent.do') < 0 ) {
		loginKakaoStartAuto();
	}else if(window.location.href.indexOf('/cSub/cEvent.do') > 0){
		loginKakaoStartAuto('snsEventJoinType');
	}else if(window.location.href.indexOf('/member/login.do') > 0 && window.document.referrer == '' ){
		console.log('kakao_sns_chk_3');
		loginKakaoStartAuto();
	}else if(window.location.href.indexOf('/member/register-intro.do') > 0 && ''.length == 6){
		loginKakaoStartAuto();
	}
}else if ( !login.isLogin && agent.indexOf('kakaotalk') >= 0 && window.document.referrer != '' ){
	if(window.location.href.indexOf('/member/register-intro.do') > 0 && '' == 'Y'){
       document.getElementById('_bodyShowViewInc').innerHTML='body > div{display:none}';
		window.onload = function() {
			loginKakaoStart();
		}
	}
}

</script>

