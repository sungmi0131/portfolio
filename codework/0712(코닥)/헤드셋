//LNB
 Vue.component('app-lnb', {
     name : "app-lnb",
     data: () => {
         return {
             cates: []
         }
     },
     created: function () {

         /*
             - json 순서와 다를 경우 -

             cateNum list

             0 - 모니터/PC 주변기기
             1 - 모바일/핸드폰 주변기기
             2 - 오디오
             3 - 카메라
             4 - iOT/주방/이미용 가전
             5 - 생활가전
             6 - 리빙
             7 - 캠핑
         */
         let cateNum = [0, 1, 4, 5, 3, 2, 6, 7];  //새로운 베열에 담아서 재정렬이 필요합니다.!!

         axios.get("../res/data/category.json").then((res) => {
             // this.cates = res.data.cateProduct
             for (var index in cateNum) {
                 // console.log(index)
                 this.cates.push(res.data.cateProduct[cateNum[index]])
             }
         });
        
     },
     computed: {
         evenNumbers: function () {
             return this.cates.filter(function (number) {
                 return number % 2 === 0
             })
         }
     },
     mounted() {
         this.slideInit();
     },
     template: `
         <div class="lnb-inner">
             <div class="swiper swiper-container lnb-slider">
                 <div class="swiper-wrapper">
                     <div class="swiper-slide">
                         <a href="../products/products-list.do?localName=Apple&items=Mac">
                             <span class="icon">
                                 <img src="../res/img/main/lnb0.png" alt="애플" class="normal"/>
                             </span>
                             <span class="text">Apple</span>
                         </a>
                     </div>
                     <div class="swiper-slide" v-for="(depth1, index) in cates" :key="index">
                         <a :href="'../products/products-list.do?localName=other&items='+depth1.title">
                             <span class="icon">
                                 <img :src="'../res/img/main/lnb'+(index+1)+'.png'" :alt="depth1.title" class="normal"/>
                             </span>
                             <span class="text">{{depth1.title}}</span>
                         </a>
                     </div>
                 </div>
             </div>
         </div>
     `,
     methods: {
         slideInit() {
             var lnbSlider = new Swiper(".lnb-slider", {
                 slidesPerView: "auto",
                 spaceBetween: 12,
                 observer: true,	// 추가
                 observeParents: true,	// 추가
             });
             setTimeout(function () {
                 lnbSlider.slideTo(0, 0, false) //슬라이드 초기화
             }, 500);
         }
     }
 });


// Products List 
Vue.component('products-list', {
    name : "products-list",
    data: () => {
        return {
            name : "Apple",
            products: null
        }
    },
    created: function () {
        axios.get("../res/data/products.json").then((res) => {
            let name = this.name;
            this.products = res.data[name]
        });
       
    },
    template: `
                <div class="products-item-list">
                    <div class="item item-" v-for="(item, index) in products" :key="index">
                        <a :href="item.href" :title="item.title">
                            <div class="item-img">
                                <img :src="item.imgsrc" :alt="item.title" />
                            </div>
                            <div class="item-text">
                                <div class="icon-wrap">
                                    <span v-for="(icon, index2) in products[index].state">{{icon[0]}}</span>
                                </div>
                                <h4>{{item.title}}<h4>

           
                            </div>
                        </a>
                    </div>
                </div>
                 `
});




