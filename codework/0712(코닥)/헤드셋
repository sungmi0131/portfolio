<script type="text/javascript">

var _bookmark_ob = [];
function getBookmark() {
	if(!login.isLogin){ return;}
	var _bookmark = localStorage.getItem('bookmark'+ btoa(login.usr_idx));
	if(_bookmark){
		_bookmark_ob = _bookmark.split(',');
	}
}
getBookmark();


function isBookmark(_ob) {
	if(!login.isLogin || !_ob || !_ob.prod_idx ){
		return false;
	}
	var _idx = _ob.prod_idx;

	if(_bookmark_ob.indexOf(''+_idx) > -1){
		return true;
	}else{
		return false;
	}
}

var _searchKeyword_ob = [];
function getSearchKeyword() {
	_searchKeyword_ob = [];
	var _searchKeyword = localStorage.getItem('searchKeyword');
	if(_searchKeyword){
		var _searchKeywordJson = JSON.parse(_searchKeyword)
		for (var i = 0; i < _searchKeywordJson.length; i++) { 	
			_searchKeyword_ob.push(_searchKeywordJson[i]); 		
		} 
		// _searchKeyword_ob = _searchKeyword.split('|');
	}
}
getSearchKeyword();

function setSearchKeyword(_searchTxt) {
	if(!_searchTxt || _searchTxt.trim() == '' ){
		return false;
	}
	_searchTxt = _searchTxt.trim();
	var _int = _searchKeyword_ob.indexOf(''+_searchTxt);
	if(_int > -1){
		_searchKeyword_ob.splice(_int, 1);
		_searchKeyword_ob.unshift(_searchTxt);
	} else {
		_searchKeyword_ob.unshift(_searchTxt);
	}
	
	if(_searchKeyword_ob.length > 30){
		_searchKeyword_ob.pop();
	}
	
	localStorage.setItem('searchKeyword', JSON.stringify(_searchKeyword_ob));
}

function delSearchKeyword(_searchTxt) {
	if(_searchTxt == '__all__'){
		_searchKeyword_ob = [];
		localStorage.setItem('searchKeyword', JSON.stringify(_searchKeyword_ob));
		return false;
	}
	if(!_searchTxt || _searchTxt.trim() == '' ){
		return false;
	}
	_searchTxt = _searchTxt.trim();
	var _int = _searchKeyword_ob.indexOf(''+_searchTxt);
	if(_int > -1){
		_searchKeyword_ob.splice(_int, 1);
	}
	localStorage.setItem('searchKeyword', JSON.stringify(_searchKeyword_ob));
}




function isNew(_date) {
	var _now = new Date();
	if( _date >=  _now.setDate(_now.getDate() - 7)  ){
		return true;
	}else{
		return false;
	}
}


Vue.filter('endTime', function(value , _data) {
	var timeout;
    if(value < new Date().getTime()){
        timeout = _data ; // "end-time"
    }else{
    }
    return timeout
});


Vue.filter('dcRate', function(value ) {
    return value.toFixed(0);
});

Vue.filter('couponType', function(value ) {
    if(value=='O' || value=='R'){
		return '오프라인';    	
    }else{
    	return '온라인';
    }
});


Vue.filter('formatDate', function(value) {
	if (value) {
		return moment(new Date(value)).format('YYYY-MM-DD')
	}
});

Vue.filter('formatDateWeek', function(value) {
	if (value) {

		var d = new Date(value);
		var week = new Array('일', '월', '화', '수', '목', '금', '토');
		
		return moment(new Date(value)).lang("ko").format('YYYY-MM-DD') + ' ('+ week[d.getDay()] + ')';
	}
});

Vue.filter('formatDate2', function(value) {
	if (value) {
		return moment(new Date(value)).format('YYYY.MM.DD')
	}
});


Vue.filter('formatDateType', function(value,c_type) {
	if (value) {
		return moment(new Date(value)).format('YYYY'+c_type+'MM'+c_type+'DD');
	}
});

	
Vue.filter('formatFullDate', function(value) {
	  if (value) {
	    return moment(new Date(value)).format('YYYY-MM-DD hh:mm')
	  }
	});

Vue.filter('imgListFile', function(value) {
	  if ( value != null && value != ''  ) {
		  // return resourceUri + value;
		  //return value.replace('.','_thumb.');
		  return common.webfile + imgListFile(value); 
	  } else {
		  // return resourceUri + 'banner/0/22/mysi84yc2q1.png';
		  // return '/static/ko/images/temp2.png';
		  
	  }
	});


Vue.filter('imgUrl', function(value) {
	  if ( value != null && value != ''  ) {
		  return resourceUri + value;
	  } else {
		  // return resourceUri + 'banner/0/22/mysi84yc2q1.png';
  		  return '/static/ko/images/temp2.png';
		  
	  }
	});

Vue.filter('checked', function(value, value2) {
	// console.log(value + ' -/- '+ value2);
		var _class = '';	
		if (value2) {
			_class = value2 + ' ';
		}
		if ( value != null && value != ''  ) {
			  return _class + 'checked';
		} else {
			return _class + '';
		}
	});

Vue.filter('isSelected', function(value1 ) {
	if ( value1  ) {
		  return true;
	  } else {
		  return false;
	  }
	});

Vue.filter('isChecked', function(value) {
	if ( value != null && value != ''  ) {
		  return true;
	  } else {
		  return false;
	  }
	});
	

Vue.filter('isNotChecked', function(value) {
	if ( value != null && value != ''  ) {
		return false;
	  } else {
		  return true;
	  }
	});

Vue.filter('num00', function(value) {
		  return ("0" + value).slice(-2);
	});

Vue.filter('formatSN', function(value) {
	var _sn = '';
	if(value.length != 16){
		return '';
	}
	for (var int = 1; int <= 4; int++) {
		_sn += _sn == '' ? '' : '-' ;
		_sn += value.substr((int-1)*4,4);
	}
	return _sn;
});

Vue.filter('premiumChk', function(value) {
	var _re = '';
	var _now = moment(new Date()).format('YYYY-MM-DD');
	var _data = moment(new Date(value)).format('YYYY-MM-DD');
//	console.log(_now);
//	console.log(_data);
	
	if(_now > _data ){
		_re = 'box-card-type3';
	}else {
		_re = 'box-card-type1';
	}
	return _re;
});

Vue.filter('premiumChk2', function(value) {
	var _re = '';
	var _now = moment(new Date()).format('YYYY-MM-DD');
	var _data = moment(new Date(value)).format('YYYY-MM-DD');
//	console.log(_now);
//	console.log(_data);
	
	if(_now > _data ){
		_re = 'prem add_bg';
	}else {
		_re = 'prem add_icon';
	}
	return _re;
});

Vue.filter('isReturn', function(value, data1, data2) {
	if(value && value != ''){
		return data1;
	} else {
		return data2;
	}
});

Vue.filter('asDateReturn', function(value, data1, data2) {
	var _now = moment(new Date()).format('YYYY-MM-DD');
	var _data = moment(new Date(value)).format('YYYY-MM-DD');
	
	var returnStr  = data1;
	if(!value || value == ''){
		//return data1;
		returnStr = data2;
	} else if( _now > _data ) {
		// return data2;
		returnStr = data2;
	} else {
		returnStr = data1;
		// return data1;
	}
	
	return returnStr;
});

Vue.filter('genderText', function(value) {
	if(value == 'M'){
		return '남성';//'男性';
	} else if(value == 'W'){
		return '여성';//'女性';
	} else {
		return '';
	}
});


Vue.filter('moneyFormat', function(c_value) {
	
	c_value = Number(c_value);
	if(c_value && c_value != ''){
		// return c_value.toString().replace(/\B(?<!\.\d*)(?=(\d{3})+(?!\d))/g, ",");	
		return c_value;
	}else{
		return '0';
	}	
	
});



Vue.filter('colorCode', function(value) {
  	if(value) {
	   	var _background = '' ;
		
  		if(colorPalette[value] && colorPalette[value] != ''){
  			_background = 'background-color:'+ colorPalette[value];
  		}else{
  			_background = 'background-color: rgb(232 232 232)';
  		}
  		return _background;
  	}
});




Vue.filter('decodeAtob', function(value) {
  	if(value) {
  		return	decodeAtob(value , 4);
  	}
});


Vue.filter('priceFormat', function(value) {
  	if(value) {
  		return String(value).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
  	}else{
  		return "0";	
  	}
});

Vue.filter('prod_url', function(_itemProd) {

	var t_cate1 = '' , t_cate2  = '' , t_cate3  = '' ;
	
	
	if(typeof _cate1 !== "undefined" && _cate1 != '' && _cate1 != null){
		t_cate1 = _cate1;

		if(typeof _cate2 !== "undefined" && _cate2 != '' && _cate2 != null){
			t_cate2 = _cate2;
			
			if(typeof cate3 !== "undefined" && cate3 != '' && _cate3 != null ){
				t_cate3 = _cate3;
			}
		}
		
	} else{
		t_cate1 = _itemProd.cate_idx1 == 0 ? '' : _itemProd.cate_idx1 ;
		t_cate2 = _itemProd.cate_idx2 == 0 ? '' : _itemProd.cate_idx2 ;
		t_cate3 = _itemProd.cate_idx3 == 0 ? '' : _itemProd.cate_idx3 ;
	}

	var _prod_rul = '../products/products-detail.do?cate1='+ t_cate1 + '&cate2='  + t_cate2 + '&cate3=' + t_cate3 + '&prod_idx=' + _itemProd.prod_idx
	return _prod_rul ;
});


