    var detailSimilar = new Vue({
        el: '.detail-similar',
        data: () => {
            return {               // data 사용 용도       || 값이 없을경우 기본값
                similardata : []     // 비슷한 상품 추천      ||            
            }
        },
        created: function () {
            axios.get("../products/recommend-prod.json?prod_idx=8007&cate2=1129&cate3=1134&type=similar&sort=B").then((res) => {
                var getType = urlParam(localNameParams);
                var getType2 = urlParam(itemsParams);
                var getType3 = urlParam(items2Params);

                this.localname = getType || "Apple";     // 값이 없을 경우 가장 기본 주소
                this.catesitemname = getType2 || "Mac";  // 값이 없을 경우 가장 기본 주소
                this.catesitemname2 = getType3 || "All"; // 값이 없을 경우 가장 기본 주소
                
                if(res.data.data){
					this.similardata = res.data.data;	
                } else{
                	this.similardata = [];
                }
                
            }).catch(err => {
                console.log(err);
            });
        },
    })

    var detailToggther = new Vue({
        el: '.detail-together',
        data: () => {
            return {               // data 사용 용도       || 값이 없을경우 기본값
                similardata: []     // 비슷한 상품 추천      ||            
            }
        },
        created: function () {
        	axios.get("../products/recommend-prod.json?prod_idx=8007&type=toggther&cate1=1069&sort=B").then((res) => {
                var getType = urlParam(localNameParams);
                var getType2 = urlParam(itemsParams);
                var getType3 = urlParam(items2Params);

                this.localname = getType || "Apple";     // 값이 없을 경우 가장 기본 주소
                this.catesitemname = getType2 || "Mac";  // 값이 없을 경우 가장 기본 주소
                this.catesitemname2 = getType3 || "All"; // 값이 없을 경우 가장 기본 주소

                if(res.data.data){
					this.similardata = res.data.data;	
                } else{
                	this.similardata = [];
                }

            }).catch(err => {
                console.log(err);
            });
        },
        
    })
    
	// 기획전
    var exhibitionSwiper = new Vue({
            el: '.exhibition-swiper',
            data: () => {
                return {               // data 사용 용도       || 값이 없을경우 기본값
					dataList: null     // 비슷한 상품 추천      ||            
                }
            },
            created: function () {
				axios.get("../data/eventProd.json?prod_idx=8007").then((res) => {
					this.dataList  = res.data.data;
					if(res.data.data.length >0 ){
						$('section.detail-exhibition').show();

					    var exhibitionSwiper = new Swiper(".exhibition-swiper", {
				            slidesPerView: 1,
