	}

	function pageTopMove(_id){
		// scroll.scrollStart();

		var $win = $('html, body');
        var $wrap = $('#wrap');
        var scrollTop = $wrap.scrollTop();
        
        if ($win.hasClass('no-scroll2')) {
            $win.removeClass('no-scroll2');
        } else {
            $win.removeClass('no-scroll');
        }
		
		if(_id){
			$('html, body').animate({
		        scrollTop: $("#"+_id).offset().top
		    }, 10);
		}else{
			$('html, body').animate({
		        scrollTop: $('body').offset().top
		    }, 10);
		}
	}	

    function addAttendEvent(_event_id) {
    	if(!login.isLogin){
    		pageMove('login');
    		return;
    	}    	
		ajaxFn('POST','../sub/addAttEventHist.do', "attend_event_idx="+_event_id,'json',function(data){
   			if(data.state == 'success'){
   				if(data.msg == "" || data.msg == null) {
  	    			alert("추가 출석 체크 하였습니다."); //입력한 출석체크 완료 메시지가 없을 시 알림
   				} else {
  	    			alert(data.msg); //입력한 출석체크 완료 메시지 표시
   				}
   				
   			}else{
   				if(data.msg == "" || data.msg == null) {
   					alert("추가 출석 체크가 실패 하였습니다.");
   				}else if(data.msg == 'NOEVENT'){
   					alert("추가 출석 이벤트 정보가 없습니다.");
   				}else if(data.msg == 'LOGIN'){
   					alert("로그인 후 가능 합니다.");
   				}else if(data.msg == 'ADDED'){
   					alert("이미 추가 출석 체크가 되어 있습니다.");
   				}else if(data.msg == 'LIMIT_ADDED'){
   					alert("최대 참여 횟수를 초과하였습니다.");
   				}else{
   	   				alert(data.msg);   				 
   				}
   			}
   		});  				 
    }	
	
