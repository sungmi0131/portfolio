	function coupon_download(){
		
		if(!login.isLogin){
			alertPopType('non_member');
			return false;
		}
		
		if(_couponList.length == 0){
			return false;
		}
		var _msg = '';
		var _coup_size = 0;
		var _use_size = 0;
		for (var int = 0; int < _couponList.length; int++) {
			if(_couponList[int].coup_prod_type == 'S' &&  _couponList[int].prod_cnt == 0 ){
				continue;
			}
			if(_couponList[int].coup_brd_type == 'S' &&  _couponList[int].brd_cnt == 0 ){
				continue;
			}
			if(_couponList[int].coup_prod_cate_type == 'S' &&  _couponList[int].prod_cate_cnt == 0 ){
				continue;
			}
			if( _couponList[int].used_dt  != null ){
				_use_size += 1;
				continue;
			}			
			$.ajax({
               type: 'POST',
               async: false,
               dataType:'json',
               url: '../mypage/insertUseCoupon.do',
               data: "prod_idx=8007&coup_idx="+_couponList[int].coup_idx,
               success: function(data) {
	   	        	if(data.state == 'success' ){
		        		console.log('쿠폰이 등록되었습니다.')
		        		// alertPopMsg('쿠폰이 등록되었습니다.');
		        		// window.location.reload();
		        	} else {
		        		console.log(data.msg)
		        		_msg = data.msg;
		        		// alertPopMsg(data.msg);
		        	}
               }
               
          	});
			_coup_size += 1;	
			
		}

		if(_coup_size == 0 ){
			if(	_use_size > 0 ){
				alertPopMsg('쿠폰 이미 사용 하였습니다.');
			}else {
				alertPopMsg('발급할 쿠폰이 없습니다.');
			}
		} else {
			if(_msg && _msg != ''){
				alertPopMsg(_msg);
			}else{
				alertPopMsg('쿠폰이 발급되었습니다.<br>마이페이지에서 확인해주세요');
			}
		}
	}	
	
    
