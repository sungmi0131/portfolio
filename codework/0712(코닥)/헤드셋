    // 카드사 확인
    var card = {
        checkObj: function (obj) {
            return $(obj).length == 0 ? false : true;
        },
        close : function(obj){
            if (!card.checkObj(obj)) {
                return;
            };
            function init(obj) {
                openSide = $(obj);
            };

            function event() {
                openSide.on('click', function (e) {
                    e.preventDefault();
                    var $this = $(this);
                    var $cardDtail = $('.card-detail');
                    $this.removeClass('active')
                    $cardDtail.slideUp();
                });
            };

            init(obj);
            event();
        },
        slideToggle : function(obj){
            if (!card.checkObj(obj)) {
                return;
            };
           function init(obj) {
                openSide = $(obj);
            };

            function event() {
                openSide.on('click', function (e) {
                    e.preventDefault();
                    var $this = $(this);
                    var $cardDtail = $('.card-detail');
                    $this.toggleClass('active')
                    $cardDtail.slideToggle();
                });
            };

            init(obj);
            event();
        }   
    } 
    $(function(){
        card.slideToggle('.card-chk-btn');
        card.close('.card-detail-close');
    })
    
    
   	/* 공통 정보 */
    function prodCommInfo(){
       	ajaxFn('POST','../products/prodCommInfo.json', 'prod_idx=8007','json',function(data){
	       	if(data.state == 'success' ){
	       		
	       		if(data.data.length > 0){
	       			
	       			var _htmlData = '';
					for (var int = 0; int < data.data.length; int++) {
						var _item = data.data[int];
						if(_item.comm_disp_cont_mo_yn && _item.comm_disp_cont_mo_yn == 'Y'){
							_htmlData += _item.comm_disp_cont_mo;
						}else{
							_htmlData += _item.comm_disp_cont;	
						}
					}	       			
		       		$('#prod_comm_info').html(_htmlData);
	       		}else{
		       		$('#prod_comm_info').hide();
	       		}
	       	}else{
	       		$('#prod_comm_info').hide();
	       	}
        })
    }
    
    prodCommInfo();
    
