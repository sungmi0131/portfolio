<script type_="text/babel">


var pageName;
/* 
  **********************************************************************************************
 url 파라미터 가져오는 함수 ie 호환용
  **********************************************************************************************
*/
var urlParam = function(name){
    var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
    if (results == null){
       return null;
    }
    else {
       return decodeURI(results[1]) || 0;
    }
}
//layout 전역 컴포넌트 - global component로 작업
//product 파라메타 name
const localNameParams = "localName";
const itemsParams = "items";
const items2Params = "items2";
const itemsCode = "itemsCode";
// Header

// 해더 참조  -  category.json 입니다.
// header 수정시 마크업은 아래에서 텍스트 또는 추가 삭제시 해당 json으로 메뉴 수정
