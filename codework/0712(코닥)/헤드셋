    }

	
	function addProdQna() {
		if(!login.isLogin){
    		alertPopType('non_member');
    		return false
    	}
		
		if($('#pop input[name=question_type]').val() == ''){
			$('#pop .question_type_msg').show();
            return false
		}
		
		
        if($('#pop input[name=question_title]').val() == ''){
        	$('#pop .question_title_msg').show();
            $('#pop input[name=question_title]').focus();
            return false
        }

        if ($('#pop textarea[name=question_cont]').val() == '') {
        	$('#pop .question_cont_msg').show();
           $('#pop textarea[name=question_cont]').focus();
           return false
       	}
        
        var url = '../products/ex-add-qna.do';
		var param = $("#pop form[name=qna_frm]").serialize();
		var actionType = $('#pop input[name=actionType]').val();
        if(actionType == 'edit'){
        	url = '../products/ex-edit-qna.do';
        }
		
		ajaxFn('post',url,param,'json' ,function(data){
  			if(data.state == 'success'){
  				if(actionType == 'edit'){
  					// alertPopMsg('상품문의가 수정되었습니다.');
  					alertPopType('qnaEdit');
  		        }else{
  		        	alertPopType('qnaWrite');
					
  		        }
  		        // popUp.close();
  			}
  		})
	}
	
	
	function bookmark(){
		$('.bookmark-button').unbind("click");
		exBookmark('8007',function(data){
			if(data.state == 'success'){
				if(data.page == 'add'){
					$('.detail-scrap-button img').attr('src','../res/img/common/cart_bookmark-icon-on.png');
				}else if(data.page == 'delete'){
					$('.detail-scrap-button img').attr('src','../res/img/common/cart_bookmark-icon.png');
				}
			}
		})
	}
	
	//선택 옵션 정보 JSON
	var optionArray = new Array(); 
	function getOption(c_depth,c_opt_name, c_ob){
		var now_depth = (c_depth - 1);
		// var ul_reset = '<li><div class="select-item-box no-value"><div class="text">옵션 선택</div></div></li>';
		var ul_reset = '';
		
		if(now_depth == 1){
			prod.opt_select_1 = c_opt_name;
   			prod.opt_select_2 = '';
   			prod.opt_select_3 = '';
   			prod.opt_select_4 = '';
   			prod.opt_select_5 = '';
   			$('#option_2_title').html(' 선택');
			$('#option_3_title').html(' 선택');
			$('#option_4_title').html(' 선택');
			$('#option_5_title').html(' 선택');
   			$('#option_2,#option_3,#option_4,#option_5').html(ul_reset).hide();
   			$('#opt_target_2,#opt_target_3,#opt_target_4,#opt_target_5').removeClass('on');
		}else if(now_depth == 2){
			prod.opt_select_2 = c_opt_name;
   			prod.opt_select_3 = '';
   			prod.opt_select_4 = '';
   			prod.opt_select_5 = '';
   			$('#option_3_title').html(' 선택');
			$('#option_4_title').html(' 선택');
			$('#option_5_title').html(' 선택');
   			$('#option_3,#option_4,#option_5').html(ul_reset).hide();
   			$('#opt_target_3,#opt_target_4,#opt_target_5').removeClass('on');
		}else if(now_depth == 3){
			prod.opt_select_3 = c_opt_name;
   			prod.opt_select_4 = '';
   			prod.opt_select_5 = '';
			$('#option_4_title').html(' 선택');
			$('#option_5_title').html(' 선택');
   			$('#option_4,#option_5').html(ul_reset).hide();
   			$('#opt_target_4,#opt_target_5').removeClass('on');
		}else if(now_depth == 4){
			prod.opt_select_4 = c_opt_name;
   			prod.opt_select_5 = '';
			$('#option_5_title').html(' 선택');
   			$('#option_5').html(ul_reset).hide();
   			$('#opt_target_5').removeClass('on');
		}else if(now_depth == 5){
			prod.opt_select_5 = c_opt_name;
		}
		
		$('#option_'+now_depth+'_title').html(c_opt_name).parent().parent().removeClass('on');
		
		var optTypePram = '';
		if(c_depth == prod.opt_cnt){
			optTypePram = '&opt_type=final';
		}

		var optPram = '';
		if(prod.opt_select_1 != ''){
			optPram = '&prod_opt_name1=' + encodeURIComponent(prod.opt_select_1);
		}
		
		if(prod.opt_select_2 != ''){
			optPram += '&prod_opt_name2=' + encodeURIComponent(prod.opt_select_2);
		}
		
		if(prod.opt_select_3 != ''){
			optPram += '&prod_opt_name3=' + encodeURIComponent(prod.opt_select_3);
		}
		
		if(prod.opt_select_4 != ''){
			optPram += '&prod_opt_name4=' + encodeURIComponent(prod.opt_select_4);
		}
		
		if(prod.opt_select_5 != ''){
			optPram += '&prod_opt_name5=' + encodeURIComponent(prod.opt_select_5);
		}
		
		c_opt_name = encodeURIComponent(c_opt_name);
		
   		var url = 'option.do';
   		var param = 'prod_idx=8007&depth='+c_depth+'&opt_name='+c_opt_name + optTypePram + optPram + '&page=detail';
   		ajaxFn('post',url,param,'json' ,function(data){
       		if(data.state == 'success'){
       			// var html = '<li><div class="select-item-box no-value"><div class="text">옵션 선택</div></div></li>';
       			var html = '';
				$(data.data).each(function(index,item){
				
					var c_fn = "";
					var c_title = "";
					var c_img_path = "";
					
					if(c_depth == prod.opt_cnt){
						// 마지막 옵션 목록  
						
						var isSoldout = false;
						var opt_name = item.opt_name // 옵션명
						if(item.opt_price != 0 && item.opt_price != ''){
							opt_name += ' (+'+ priceFormat(item.opt_price) + '원)' ; 	// 옵션명
						}
						
						var opt_stock = item.opt_stock;
						// if(prod.stock_yn == 'Y'){
						if(prod.stock_yn == 'N'){
							opt_stock = 9999;			
						}
						
						var optSelectFn = 'selectOption('+(Number(c_depth) + 1)+',\''+item.opt_name+'\' ,\''+item.prod_opt_item_idx+'\' ,\''+opt_stock+'\' ,\''+item.opt_price+'\' ,this)';
						
						if(prod.soldout_yn == 'Y' || item.opt_sold_out_yn == 'Y' || opt_stock <= 0 ){
							//품절
							isSoldout = true;
							opt_name = item.opt_name + '[품절]';
							// optSelectFn = 'alertPopMsg(\'품절된 상품입니다.\');';
							optSelectFn = 'alertPopMsg(\'품절된 상품입니다.\');';
						}
						if(c_depth == 1){
							c_img_path = item.img_path;
						}
						c_fn = optSelectFn;
						c_title = opt_name;
					}else{
						
						if(c_depth == 1){
							c_fn = 'getOption('+(Number(c_depth) + 1)+',\''+item.opt_value+'\' ,this)';
							c_title = item.opt_value;
							c_img_path = item.img_path;
						}else{
							c_fn = 'getOption('+(Number(c_depth) + 1)+',\''+item+'\' ,this)';
							c_title = item;
						}
					}
						
					if(c_depth == 1){
						html += getOptionMakeHtml(c_fn,c_title,c_img_path);
					}else{
						html += getOptionMakeHtml(c_fn,c_title,'');
					}
						
					
				})
				$('#option_'+c_depth).html(html);
				selectBox();
       		}
       	})
       	
       	if(c_ob){
       		$(c_ob).parent().parent().hide();
       	}
       	
   	}
   		
	
	function getOptionMakeHtml(c_fn,c_title,c_img_path){
		var html = '';
		html += ' <li > ';
		html += '     <div class="select-item-box" onclick="'+c_fn+'" >  ';
		if(c_img_path != null && c_img_path != '' ){
			html += ' <div class="img"><img src="'+common.webfile+c_img_path+'" alt="'+c_title+'"></div> ';
		}		
		html += '          <div class="text"> ';
		html += '              '+c_title+' ';
		html += '          </div> ';
		html += '      </div> ';
		html += ' </li> ';
		return html;
	}
	
	
	
	function selectOption(c_depth,c_opt_name,c_idx,c_stock,c_price,c_ob){
		var now_depth = (c_depth - 1);
		// $('#option_'+now_depth+'_text').html(c_opt_name);
		
		if('Y' == 'Y'){
			console.log('------------------------------------ ' + $('.amount_chk').length);
			var _buy_max_total = 0;
			var _buy_max_end = false;
			$('.amount_chk').each(function (index, item) {
			    if(isNaN(item.value)){
			    	alert('잘못된 상품 수량 정보가 있습니다.');
			    	_buy_max_end = true;
			    	return;
			    }else{
			    	_buy_max_total += Number(item.value);	
			    }
				
			});	
			
		     console.log(_buy_max_total);
			
		     if(_buy_max_end){
				$('#option_text_'+now_depth).html('<span class="txt" style="position: relative;">선택해 주세요</span>');
		    	 return;
		     }
			if( Number('1') <= ordSum + _buy_max_total){
				// alert('최대 구매 수량이 초과 하였습니다.');
				alert('해당 상품은 최대 1개 까지 구매 가능합니다.');
				$('#option_text_'+now_depth).html('<span class="txt" style="position: relative;">선택해 주세요</span>');
				return false;
			}
			
			if(Number('1') <= ordSum + _buy_max_total + cartSum){
				alert('해당 상품은 최대 1개 까지 구매 가능합니다.\n장바구니 담은 상품 포함 최대 구매 수량이 초과 하였습니다.\n장바구니 상품 삭제 후 선택해 주세요.');
				$('#option_text_'+now_depth).html('<span class="txt" style="position: relative;">선택해 주세요</span>');
				return false;
			}
			
		}
		
		if(c_price == '' || c_price == '0'){
			$('#option_'+now_depth+'_title').html(c_opt_name).parent().parent().removeClass('on');
		}else{
			$('#option_'+now_depth+'_title').html(c_opt_name + '(+'+priceFormat(c_price)+'원)').parent().parent().removeClass('on');
		}
		// $('#option_'+now_depth+'_title').html(c_opt_name).parent().parent().removeClass('on');
		
		if(now_depth == 1){
			prod.opt_select_1 = c_opt_name;
		}else if(now_depth == 2){
			prod.opt_select_2 = c_opt_name;
		}else if(now_depth == 3){
			prod.opt_select_3 = c_opt_name;
		}else if(now_depth == 4){
			prod.opt_select_4 = c_opt_name;
		}else if(now_depth == 5){
			prod.opt_select_5 = c_opt_name;
		}
		
		prod.opt_select_idx = c_idx;
		prod.opt_select_price = c_price;
		prod.opt_stock = c_stock;
		
		if(c_ob){
       		$(c_ob).parent().parent().hide();
       	}
		 
		if(prod.opt_input_yn == 'N'){
			choiceProd(c_price);

			$('#option_2 li:gt(0)').remove();
			$('#option_3 li:gt(0)').remove();
			$('#option_4 li:gt(0)').remove();
			$('#option_5 li:gt(0)').remove();
		}		
		
		
	}
	
	
	function setProdAdd(c_id , c_name , c_price,c_stock, buy_min, buy_max_yn, buy_max,c_ob){
		
		if($('input[name=add_amount_'+ c_id +']').is(':visible')){
			alert('이미 선택된 상품입니다.');			
			return;
		}
		var c_html = '<input type="hidden" name="add_prod_idxs" value="'+ c_id + '">';
		c_html += '<input type="hidden" name="add_prod_idx_'+c_id+'" value="'+c_id+'">';
		c_html += '<input type="hidden" class="add_prod_cnt">';
		/*
			--> 최대 구매수량 10개로 지정 , 작업 필요함
		*/
		var html = makeProdHtml(c_id , c_name , c_price, c_html,c_stock,'Y', buy_min, buy_max_yn, buy_max , 0);
			
		if(c_ob){
       		$(c_ob).parent().parent().hide();
       		var $h4_ob =  $(c_ob).parent().parent().parent().find('h4');
       		$h4_ob.removeClass('on');
       		// $h4_ob.removeClass('on').find('span').html($h4_ob.attr('title'));
       	}
	}
	
	
	
	function choiceProd(c_price){
		// $('.option-list-input-btn').unbind("click");
	
		var buy_min = '0';
		var buy_max_yn = 'Y';
 		var buy_max = '1';
		
		if(prod.opt_yn == 'Y'){
			if(prod.opt_cnt >= 1){
				if(prod.opt_select_1 ==''){
					alertPopMsg('를 선택해 주세요.');
					return false;
				}  
			}
			if(prod.opt_cnt >= 2){
				if(prod.opt_select_2 ==''){
					alertPopMsg('를 선택해 주세요.');
					return false;
				}  
			}
			if(prod.opt_cnt >= 3){
				if(prod.opt_select_3 ==''){
					alertPopMsg('를 선택해 주세요.');
					return false;
				}  
			}
			if(prod.opt_cnt >= 4){
				if(prod.opt_select_4 ==''){
					alertPopMsg('를 선택해 주세요.');
					return false;
				}  
			}
			if(prod.opt_cnt == 5){
				if(prod.opt_select_5 ==''){
					alertPopMsg('를 선택해 주세요.');
					return false;
				}  
			}
			
			var prod_opt_item_idx = prod.opt_select_idx;
   			
			
			
			isProdOverlap = false;	       			
   			$('input[name=prod_opt_item_idxs]').each(function(){
			    console.log($(this).val());
			    var value = $(this).val();
			    if(value == prod_opt_item_idx){
			    	isProdOverlap = true;
			    	return false;
			    }
			})
			if(isProdOverlap){
				alertPopMsg('이미 등록된 옵션상품입니다.');
				
				prod.opt_select_1 = '';
       			prod.opt_select_2 = '';
       			prod.opt_select_3 = '';
       			prod.opt_select_4 = '';
       			prod.opt_select_5 = '';
       			
       			if( Number(prod.opt_cnt) >= 1){
       				$('#option_1_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 2){
       				$('#option_2_title').html(' 선택');		
       			}
       			if( Number(prod.opt_cnt) >= 3){
       				$('#option_3_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 4){
       				$('#option_4_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 5){
       				$('#option_5_title').html(' 선택');	
       			}
       			
				return false;
			}
   			
   			
   			// var opt_price = Number(prod.opt_select_price) + Number('499000');
   			var opt_name = '';
   			var opt_stock = prod.opt_stock;
   			
   			for (var i = 1; i <= prod.opt_cnt; i++) {
				if(i == 1){
					opt_name = prod.opt_select_1;  
				}else if(i == 2){
					opt_name += ' / ' + prod.opt_select_2;
				}else if(i == 3){
					opt_name += ' / ' + prod.opt_select_3;
				}else if(i == 4){
					opt_name += ' / ' + prod.opt_select_4;
				}else if(i == 5){
					opt_name += ' / ' + prod.opt_select_5;
				}
			}
   			
   			var c_html = '<input type="hidden" name="prod_opt_item_idx_'+prod_opt_item_idx+'" value="'+ prod_opt_item_idx + '">'
   			c_html += '<input type="hidden" name="prod_opt_item_idxs" value="'+ prod_opt_item_idx + '">'
   			if(prod.opt_input_yn == 'Y'){
   				c_html += '<input type="hidden" name="ord_opt_input_value_'+prod_opt_item_idx+'" value="'+$('#popTextarea').val()+'">'
   				if(opt_name != '' ){
   					opt_name += ' / ' + $('#popTextarea').val();
   				}else{
   					opt_name = $('#popTextarea').val();	
   				}
   				$('#popTextarea').val('');
   			} 
   			
   			if(c_price && c_price != '' && c_price != '0' ){
   				opt_name = opt_name +'(+' + c_price.replace(/\B(?=(\d{3})+(?!\d))/g, ",") + '원)';
   			}
   			
   			// 선택한 옵션 정보 
   			var html =	makeProdHtml(prod_opt_item_idx , opt_name , prod.opt_select_price, c_html, opt_stock,'N', buy_min, buy_max_yn, buy_max, '0');
		}else{
			var opt_name = '';
			var c_html = '';
   			if(prod.opt_input_yn == 'Y'){
   				c_html += '<input type="hidden" name="ord_opt_input_value" value="'+$('#popTextarea').val()+'">';
   				if(opt_name != '' ){
   					opt_name += ' / ' + $('#popTextarea').val();
   				}else{
   					opt_name = $('#popTextarea').val();	
   				}
   				$('#popTextarea').val('');
   			} 
   			var html =	makeProdHtml('0' , opt_name , '0' , c_html , prod.stock,'N', buy_min, buy_max_yn, buy_max , '0');
		}
	}
	
	
	
	function makeProdHtml(c_id , c_name , c_price, c_input , c_stock , c_add_yn, buy_min, buy_max_yn, buy_max, useOrdCount){
		if(buy_min == '' || buy_min == '0') {buy_min = 1;}
		if(buy_max == '' || buy_max == '0') {buy_max = 1;}
		if(buy_max_yn == 'Y' ){ 
			if(useOrdCount && useOrdCount != '' ){
				buy_max = Number(buy_max) - Number(useOrdCount);
				if(buy_max < 0 ){
					buy_max  = 0;
					alertPopMsg('구매 수량을 초과 하였습니다. ');
					return;
				}
			}
			if( c_stock > buy_max ) c_stock = buy_max
		};
		
		
		
		console.log('------------------------------------->');
		console.log('------------------------------------->');
		console.log(prod.price);
		console.log('c_price : ' + c_price);
		
		
		// var prod_price = Number(prod.price) + Number(c_price);
		var prod_price;
		if(c_add_yn == 'Y'){
			prod_price =  Number(c_price);
		}else{
			prod_price = Number(prod.price) + Number(c_price);	
		}
		
		var html = '';
			html += '<div class="order-list-box"> ';
			html += '' + c_input;
			html += '	<a href="" class="del-btn del-btn-s"><img src="../res/img/common/close-icon.png" alt="삭제"></a>  ';
			html += '	<h5 class="fs12 c-pc1">'+ c_name + '</h5>  ';
			html += '	<div class="order-list-box-inner">  ';
			html += '		<div class="amount-box" >  ';
			html += '			<button type="button" class="amount-control amount-minus" data-min="'+buy_min+'" data-price="'+prod_price+'" ></button>  ';
			if(c_add_yn == 'N'){
				if(prod.opt_yn == 'Y'){
					html += '			<input class="amount_chk" type="number" name="amount_'+c_id+'"  value="' +buy_min+ '" readonly="readonly">  ';
				}else{
					html += '			<input class="amount_chk" type="number" name="amount"  value="' +buy_min+ '" readonly="readonly">  ';
				}	
			}else{
				html += '			<input type="number" name="add_amount_'+c_id+'" buy_max_yn="'+buy_max_yn+'"  value="'+ buy_min +'" readonly="readonly">  ';
			}
			html += '			<button type="button" class="amount-control amount-plus" data-max="'+c_stock+'" buy_max="'+buy_max+'" buy_max_yn="'+buy_max_yn+'" data-price="'+prod_price+'" ></button>  ';
			html += '		</div>  ';
			html += '		<div class="price font-kr-bold fs14 c-sc8"> '; 
			html += '			<span class="lato-bold fs16 f-w8 price_sum">'+ priceFormat(prod_price) + '</span>원 ' ;
			html += '		</div>  ';
			html += '	</div>  ';
			html += '</div>  ';
			
			$('#prod_form_1').append(html);
				// $('#popTextarea').val('');
				
				prod.opt_select_1 = '';
				prod.opt_select_2 = '';
				prod.opt_select_3 = '';
				prod.opt_select_4 = '';
				prod.opt_select_5 = '';
				// $('#option_text_1,#option_text_2,#option_text_3,#option_text_4,#option_text_5').html(selectMsg);
				if( Number(prod.opt_cnt) >= 1){
       				$('#option_1_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 2){
       				$('#option_2_title').html(' 선택');		
       			}
       			if( Number(prod.opt_cnt) >= 3){
       				$('#option_3_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 4){
       				$('#option_4_title').html(' 선택');	
       			}
       			if( Number(prod.opt_cnt) >= 5){
       				$('#option_5_title').html(' 선택');	
       			}
				
				calculatePrice();		// 총 주문금액 계산
				amount();
		return html;
	}
	
	
	 
    function exPurchase(){
    	console.log('------> 바로구매 !!');
    	addCart('Y');
    }
    
    function addCart(c_type){
    	console.log('------> 장바구니 담기 !!');
    	
    	
    	var pcnt = $('#prod_form_1 .order-list-box').length;
    	if(pcnt == 0){
    		alertPopMsg('상품을 선택해 주세요');
    		return false;
    	}else{
    		
    		
    		var add_prod_cnt = $('.add_prod_cnt').length;
    		if(add_prod_cnt == pcnt){
    			alertPopMsg('상품을 선택해 주세요');
        		return false;
    		}
    		if(prod.opt_yn == 'N' && prod.opt_input_yn == 'Y' ){
    			var ord_opt_input_value_arr = '';
    			var amount_arr = '';
    			$('input[name=ord_opt_input_value]').each(function(){
    				if(ord_opt_input_value_arr == ''){
    					ord_opt_input_value_arr = $(this).val();
    				}else{
    					ord_opt_input_value_arr = ord_opt_input_value_arr + '|&|' + $(this).val();
    				}
    			})
    			
    			$('input[name=amount]').each(function(){
    				if(amount_arr == ''){
    					amount_arr = $(this).val();
    				}else{
    					amount_arr = amount_arr + '|&|' + $(this).val();
    				}
    			})
    			
    			$('input[name=ord_opt_input_value_arr]').val(ord_opt_input_value_arr);
    			$('input[name=amount_arr]').val(amount_arr);
    		}
    		
    		if('Y' == 'Y'){
    		
				var _buy_max_total =0;
	    		
				$('.amount_chk').each(function (index, item) {
			    	_buy_max_total += Number(item.value);	
				});	
	    		
				if( Number('1') < ordSum + _buy_max_total){
					alert('해당 상품은 최대 1개 까지 구매 가능합니다.');
					return false;
				}
				if(c_type != 'Y'){
					if(Number('1') < ordSum + _buy_max_total + cartSum){
						alert('해당 상품은 최대 1개 까지 구매 가능합니다.\n장바구니 담은 상품 포함 최대 구매 수량이 초과 하였습니다.\n장바구니 상품 삭제 후 선택해 주세요.');
						return false;
					}
				}
				
    		}
    		
    		$('input[name="prod_cnt"]').val(pcnt);
    		$('input[name="direct_buy_yn"]').val(c_type);
        	var _url = '../products/addCart.do';
        	var _params = $("form[name=prod_form_1]").serialize() ;
        	
        	ajaxFn('post',_url,_params,'json' ,function(data){
        		if(data.state == 'success'){
        			if(c_type == 'Y'){
        				//주문서 생성
            			makeCartGroup(data.data);
                	}else{
                		if(prod.opt_yn == 'Y'){
                			// 장바구니 담기 완료 후 선택된 상품 초기화
                			//$('#prod_form_1 .order-list-box').remove();	  
                		}
                		alertPopType('cart');
                	}
        			
        			var _a_sum = 0;
        			$('.amount-box input[type=number][name^=amount]').map(function(i,v){ _a_sum += Number($(this).val()) ;});
        			try{AW_PRODUCT(_a_sum);}catch(e){}
        			
        		}
        	})	
    	}
    }
	
	
	
	function calculatePrice(){
		// 총 주문금액 계산 
        var total_price = 0;
        $('.price_sum').each(function(){
        	console.log(minusComma($(this).html()))
        	total_price += Number(minusComma($(this).html()));
        })
        $('#total_price').html(addComma(String(total_price)));
	}
	
	
	function amount() {
	    var $amountBox = $('.amount-box')
	    if (!$amountBox.length) return;
	    function amountHandler(e) {
	       var $this = $(e.target);
	        if ($this.hasClass('amount-minus')) {
	            var stat = $this.next('input').val();
	            var num = parseInt(stat,10);
	                num--;
	                if(num<=0){
	                    // alert('더이상 줄일수 없습니다.');
	                num =1;
	            }
	            $this.next('input').val(num);
	            var price = $this.attr('data-price');
	            $this.parents('.order-list-box-inner').find('.price_sum').html(addComma(String(Number(price) * num)));
	        } else {
	            var max = $this.attr('data-max') || null;
	            var stat = $this.prev('input').val();
	            var num = parseInt(stat,10);
	                num++;
	                
	            if( $this.attr('buy_max_yn') == 'Y'){
	            	
	    			var _buy_max_total =0;
		    		
					$('.amount_chk').each(function (index, item) {
				    	_buy_max_total += Number(item.value);	
					});	
		    		
					if (max != null || max != undefined) {
		                if(num>max){
	                    	alert('현재 상품은 ' + max + '개까지 구매가 가능 합니다. ');
		                    return false;
		                }    
		            }
					
					if( Number($this.attr('buy_max')) <= ordSum + _buy_max_total){
						alert('해당 상품은 최대 '+Number($this.attr('buy_max')) +'개 까지 구매 가능합니다.');
						return false;
					}
					
					if(Number($this.attr('buy_max')) <= ordSum + _buy_max_total + cartSum){
						alert('해당 상품은 최대 '+Number($this.attr('buy_max')) +'개 까지 구매 가능합니다.\n장바구니 담은 상품 포함 최대 구매 수량이 초과 하였습니다.\n장바구니 상품 삭제 후 선택해 주세요.');
						return false;
					}
	            
	            }
	                	                

	            // 최대수량 있을 경우 data-max
	            if (max != null || max != undefined) {
	                if(num>max){
	                    // alertPop("amountMax", max)
	                    if( $this.attr('buy_max_yn') == 'Y'){
	                    	alertAmountMaxPop($this.attr('buy_max'));
	                    }else{
	                    	alert('현재 상품은 ' + max + '개까지 구매가 가능 합니다. ');
	                    }
	                    
	                    num=max;
	                }    
	            }
	            
	            $this.prev('input').val(num);
	            var price = $this.attr('data-price');
	            $this.parents('.order-list-box-inner').find('.price_sum').html(addComma(String(Number(price) * num)));
	        }
	        
	        calculatePrice();		// 총 주문금액 계산 
	        
	    }
	    $amountBox.each(function () {
	    	$(this).find('.amount-control').off('click');
	        $(this).find('.amount-control').on('click' , amountHandler);
	    })
	}
	
	/* 상품 필수 정보  */
	function essentialData(){
		var json_data_info = '';
		var prod_Info_memo  = $('#essen_item_data').val() ;  
		if(prod_Info_memo != null && prod_Info_memo != "" ){
			json_data_info = JSON.parse(prod_Info_memo);
			
			var html = `
				<colgroup>
				    <col width="28%">
				    <col width="">
				</colgroup>
			`;
			
			$(json_data_info).each(function(index,item){
				if(item.name != ''){
					html += '<tr>';
					html += '	<th>'+item.name+'</th> ';
					html += '	<td>'+item.value+'</td> ';
					html += '</tr>';
					$('#table-drop1').html(html);
				}
			})
		}
	}


</script>




