	function ajaxFn(_type,_url,_params,_dataType ,_call ){
		$.ajax({ 
			type: _type,
		    url: _url, 
		    data: _params ,
		    dataType:_dataType,
		    success: function (_data) {
		    	_call(_data);	
		    },
		    error:function(e){  
		    	_call('error');
	        }
		});
	}
	
	
	function ajaxAsyncFn(_type,_url,_params,_dataType ,_call ){
		$.ajax({ 
			type: _type,
		    url: _url, 
		    data: _params ,
		    async: false,
		    dataType:_dataType,
		    success: function (_data) {
		    	_call(_data);	
		    },
		    error:function(e){  
		    	_call('error');
	        }
		});
	}
	
	
	function Go(c_url) {
	    window.location.href=c_url;
	}

	function newGo(c_url) {
		window.open(c_url);
	}

	function priceFormat(num){
		return String(num).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
	}
	
	
	
	function makeCartGroup(c_ids){
    	var _url = '../products/addOrd_cart_grp.do';
    	var _params = "ord_cart_idxs=" +c_ids ;
    	ajaxFn('post',_url,_params,'json' ,function(data){
    		if(data.state == 'success'){
    			Go('../order/order.do?ord='+data.data);
    		}
    	})
    }

	// 우편번호 찾기 화면을 넣을 element
	var element_layer = document.getElementById('layer');

	// iframe을 넣은 element를 안보이게 한다.
    function closePostcode() {
        element_layer.style.display = 'none';
    }
	
	// 브라우저의 크기 변경에 따라 레이어를 가운데로 이동시키고자 하실때에는
    // resize이벤트나, orientationchange이벤트를 이용하여 값이 변경될때마다 아래 함수를 실행 시켜 주시거나,
    // 직접 element_layer의 top,left값을 수정해 주시면 됩니다.
    function initLayerPosition(){
		var width= window.innerWidth;
		var height = window.innerHeight;
       	// var width = 300; //우편번호서비스가 들어갈 element의 width
       	// var height = 400; //우편번호서비스가 들어갈 element의 height
        var borderWidth = 5; //샘플에서 사용하는 border의 두께

        // 위에서 선언한 값들을 실제 element에 넣는다.
        element_layer.style.width = width + 'px';
        element_layer.style.height = height + 'px';
        element_layer.style.border = borderWidth + 'px solid';
        // 실행되는 순간의 화면 너비와 높이 값을 가져와서 중앙에 뜰 수 있도록 위치를 계산한다.
        //element_layer.style.left = (((window.innerWidth || document.documentElement.clientWidth) - width)/2 - borderWidth) + 'px';
        //element_layer.style.top = (((window.innerHeight || document.documentElement.clientHeight) - height)/2 - borderWidth) + 'px';
        element_layer.style.left = '0px';
        element_layer.style.top = '0px';
	}

	
	
	function postcode() {
		new daum.Postcode({
            oncomplete: function(data) {
                var fullAddr = data.address; 	// 최종 주소 변수
                var extraAddr = ''; 			// 조합형 주소 변수
                if(data.addressType === 'R'){
                    if(data.bname !== ''){
                        extraAddr += data.bname;
                    }
                    if(data.buildingName !== ''){
                        extraAddr += (extraAddr !== '' ? ', ' + data.buildingName : data.buildingName);
                    }
                    fullAddr += (extraAddr !== '' ? ' ('+ extraAddr +')' : '');
                }
                document.getElementById('post').value = data.zonecode; //5자리 새우편번호 사용
                document.getElementById('addr').value = fullAddr;
                // document.getElementById('address2').focus();
                element_layer.style.display = 'none';
                
                // if(getDeliv_type){getDeliv_type(data.zonecode)};
                if(window.document.location.href.indexOf('/products-detail.do') > -1 || window.document.location.href.indexOf('/order.do') > -1) {
	            	getDeliv_type(data.zonecode);
	            }
            },
            width : '100%',
            height : '100%',
            maxSuggestItems : 5
        }).embed(element_layer);
        
        // iframe을 넣은 element를 보이게 한다.
        element_layer.style.display = 'block';

        // iframe을 넣은 element의 위치를 화면의 가운데로 이동시킨다.
        initLayerPosition();
	}
	
	
	function postcodeType(c_id) {
		
		console.log(' -------------------------------------> ');
		console.log('c_id : '+ c_id);
		
	    new daum.Postcode({
            oncomplete: function(data) {
                var fullAddr = data.address; 	// 최종 주소 변수
                var extraAddr = ''; 			// 조합형 주소 변수
                if(data.addressType === 'R'){
                    if(data.bname !== ''){
                        extraAddr += data.bname;
                    }
                    if(data.buildingName !== ''){
                        extraAddr += (extraAddr !== '' ? ', ' + data.buildingName : data.buildingName);
                    }
                    fullAddr += (extraAddr !== '' ? ' ('+ extraAddr +')' : '');
                }
                
                
                console.log('data.zonecode : ' + data.zonecode)
                console.log('c_id : ' + c_id)
                console.log('fullAddr : ' + fullAddr)
                
                
                document.getElementById('post'+c_id).value = data.zonecode; //5자리 새우편번호 사용
	            document.getElementById('addr'+c_id).value = fullAddr;
                // document.getElementById('address2').focus();
                element_layer.style.display = 'none';
                
                // if(getDeliv_type){getDeliv_type(data.zonecode)};
                if(window.document.referrer.indexOf('/products-detail.do') > -1){
	            	getDeliv_type(data.zonecode);
	            }
            },
            width : '100%',
            height : '100%',
            maxSuggestItems : 5
        }).embed(element_layer);
        
        // iframe을 넣은 element를 보이게 한다.
        element_layer.style.display = 'block';

        // iframe을 넣은 element의 위치를 화면의 가운데로 이동시킨다.
        initLayerPosition();
	}

	function nextViewMenu(_ob, _num){
		var _num_data = 0;
		var _data = $(_ob);
		if(_num){
			_num_data =_num;
		}
		
		for (var int = 0; int < _num_data; int++) {
			var _data = _data.parent();
		} 
		_data.next().slideToggle();
		
	}
	
	
	
	function changeOption(c_this) {
    	var prod_opt_item_idx = $(c_this).attr('prod_opt_item_idx');
    	var prod_idx = $(c_this).attr('prod_idx');
    	var ord_opt_input_yn = $(c_this).attr('ord_opt_input_yn');	
    	var ord_opt_input_title = $(c_this).attr('ord_opt_input_title');	
    	var ord_opt_input_value = $(c_this).attr('ord_opt_input_value');
    	var opt_price = $(c_this).attr('opt_price');
    	
    	
    	
    	$('#option_change_from input[name=prod_opt_item_idx]').val(prod_opt_item_idx);
		$('#option_change_from input[name=prod_idx]').val(prod_idx);
		$('#option_change_from input[name=ord_opt_input_yn]').val(ord_opt_input_yn);
		$('#option_change_from input[name=ord_opt_input_title]').val(ord_opt_input_title);
		$('#option_change_from input[name=ord_opt_input_value]').val(ord_opt_input_value);
		$('#option_change_from input[name=opt_price]').val(opt_price);
		
		
		var params = $("form[name=option_change_from]").serialize();
        var url  = "../order/pop-option-change.do";
		$.ajax({
		    type: "POST", //요청 메소드 방식
		    url: url,
		    data : params,
		    dataType: "text", //서버가 요청 URL을 통해서 응답하는 내용의 타입
		    success: function (result) {
		    	console.log(result);
		    	$('#option_popup').html(result);
		    	var LayerPopup = $(".option-layer-wrap");		
		        LayerPopup.addClass('show');
		        $("html, body").addClass("no-scroll")
		    },
		    error: function (a, b, c) {}
		});
    }
	
	
	function getBookmark2(_skip) {
		if(!login.isLogin && !_skip && _skip != 'Y' ){
			return false
		}
		var param = '';
		var url = '../products/get_bookmark.do';
		ajaxAsyncFn('post',url,param,'json' ,function(data){
				if(data.state == 'success'){
					console.log(data.data.prod_mark_idxs);
	
					if(data.data.prod_mark_idxs){
		  				_bookmark_ob = data.data.prod_mark_idxs.split(',');
					}else{
						_bookmark_ob = [];
					}
					
					localStorage.removeItem('bookmark'+ btoa(login.usr_idx));
					localStorage.setItem('bookmark'+ btoa(login.usr_idx),data.data.prod_mark_idxs);
				}
		})
	}	

	
	function exBookmark(c_prod_idx,_call) {
		if(!login.isLogin){
    		alertPopType('non_member');
    		return false
    	}
		var param = 'prod_idx=' + c_prod_idx;
		var url = '../products/ex-process-bookmark.do';
		ajaxAsyncFn('post',url,param,'json' ,function(data){
  			if(data.state == 'success'){
  				console.log(data.data.prod_mark_idxs);

  				if(data.data.prod_mark_idxs){
  	  				_bookmark_ob = data.data.prod_mark_idxs.split(',');
  				}else{
  					_bookmark_ob = [];
  				}
  				
  				localStorage.removeItem('bookmark'+ btoa(login.usr_idx));
  				localStorage.setItem('bookmark'+ btoa(login.usr_idx),data.data.prod_mark_idxs);
  			}
  			_call(data);
  		})
	}
	
	
	
