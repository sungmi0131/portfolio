    function alertAmountMaxPop(max){
    	var $body = $('body');
	    var popId = 'alertPop';
	    var popHtml = "";
	    if ($('#'+ popId).length) {
	        $('#'+ popId).remove();
	    }
	    $body.append('<div id="'+popId+'"/>').find('#'+ popId).html('<div class="alert" />');
    	popHtml = '';
    	popHtml += '    <div class="alertInner"> ';
    	popHtml += '        <div class="text"> ';
    	popHtml += '            <p class="slg">해당 상품은 최대 '+ max +'개 까지 구매 가능합니다.</p>  ';
    	/* popHtml += '            <p class="slg">해당 상품은 '+ max +'개이상</p> ';
    	popHtml += '            <p class="slg">주문하실 수 없습니다.</p> ';
    	popHtml += '            <p class="slg">1회에 최대로 주문가능한</p> ';
    	popHtml += '            <p class="slg">수량으로 변경해주세요.</p> '; */
    	popHtml += '        </div> ';
    	popHtml += '        <div class="btn-wrap"> ';
    	popHtml += '            <button onclick="alertClose()" class="btn btn-type-s-4 btn-single btn-color-type1">확인</button> ';
    	popHtml += '        </div> ';
    	popHtml += '     </div> ';
    	$('#alertPop').find('.alert').html(popHtml);
    }

