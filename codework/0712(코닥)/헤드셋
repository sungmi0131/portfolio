		$('.list-empty').show();
	}


	/* 
	  **********************************************************************************************
	  custom amount  - 커스텀 숫자 카운트 가이드 부분 입니다.
	  * 업데이트시 해당 칸에 업데이트 날짜 주석 처리 * 
	  **********************************************************************************************
	*/
	function amount2() {
	    var $amountBox = $('.amount-box')
	    if (!$amountBox.length) return;

	    function amountHandler(e) {
	       var $this = $(e.target);
	        if ($this.hasClass('amount-minus')) {
	        	var min = $this.attr('data-min') || '0';
	        	if(min == ''){
	        		min = 0 ;
	        	}else{
	        		min = Number(min);
	        	}
	        	
	        	var stat = $this.next('input').val();
	            var num = parseInt(stat,10);
	                num--;
	                if(num <= min -1 ){
	                    alert('더이상 줄일수 없습니다.');
	                num = min;
	            }
	            $this.next('input').val(num);
	        } else {
	            var max = $this.attr('data-max') || null;
	            var stat = $this.prev('input').val();
	            var num = parseInt(stat,10);
	                num++;

	            // 최대수량 있을 경우 data-max
	            if (max != null || max != undefined) {
	                if(num>max){
	                    alertPop("amountMax", max)
	                    num=max;
	                }    
	            }
	            
	            $this.prev('input').val(num);
	        }
	    }
	    $amountBox.each(function () {
	        $(this).find('.amount-control').on('click' , amountHandler);
	    })

	}
	
	/* 윈도우 팝업 */
	function openWindowPopup(url, winName, width, height){
		var wLeft = window.screenLeft ? window.screenLeft : window.screenX;
		var wTop = window.screenTop ? window.screenTop : window.screenY;
		var left = wLeft + (window.innerWidth / 2) - (width / 2);
		var top = wTop + (window.innerHeight / 2) - (height / 2);
		return window.open(url, winName, "width=" + width + ",height=" + height + ",left=" + left + ",top=" + top + ",scrollbars=yes");
	}
	
	
	function incAddCart(c_id,c_cnt,c_type){
		var _url = '../products/addCart.do';
		var _params = 'prod_idx='+c_id + '&amount='+c_cnt+'&direct_buy_yn=N&prod_cnt=1&ord_opt_input_yn=N';
		ajaxFn('post',_url,_params,'json' ,function(data){
			if(data.state == 'success'){
				if(c_type == 'M'){
					alertPopType('cart');	
				}else if(c_type == 'N'){
					alertPopMsg('선택하신 상품이 장바구니에 담겼습니다.','window.location.reload()');
					
				}
	       		
			}
		})
	}

	if(!login.isLogin && getCookie('auto_login_id') !='' ){
    	var _url = '../member/autoLoginAction.do';
    	var _params = "";
    	ajaxFn('post',_url,_params,'json' ,function(data){
    		if(data.state == 'success'){
    			getBookmark2('Y');
    			window.location.reload();
    		}
    	})
	}
	
	
	function pnNumberFormat(string){
		var returnPn = '';
		if(string != ''){
			returnPn = string.replace(/^(\d{2,3})(\d{3,4})(\d{4})$/, `$1-$2-$3`);	
		}
		return returnPn;
	}
	
	
	function pnNumberFormatType(string,c_type){
		var returnPn = '';
		if(string != '' && string != '010'){
			var pnText;
			if(string.indexOf('-') > -1){
				pnText = string;
			}else{
				pnText = string.replace(/^(\d{2,3})(\d{3,4})(\d{4})$/, `$1-$2-$3`);
			}
			returnPn = pnText.split('-');
			return returnPn[c_type];
		}else{
			return returnPn;
		}
	}
	
	
	
	function pageBack(c_back_type){
		if(c_back_type != ''){
			if(c_back_type == '1'){
				window.history.go(-1);	
			}else if(c_back_type == '2'){
				window.history.go(-2);	
			}else if(c_back_type == '3'){
				window.history.go(-3);		
			}else{
				window.history.back();	
			}
		}else{
			window.history.back();
		}
	}

	
	
	function pageMove(c_page,c_url){
		if(c_page == 'main'){
			window.location.href = '../main/main.do';
		}else if(c_page == 'mypage'){
			window.location.href = '../mypage/index.do';
		}else if(c_page == 'login'){
			var params = '';
			var urlParams = new URLSearchParams(window.location.search);
			if(urlParams.has('returnUrl')){
				console.log(1)
				params = window.location.search;
			}else{
				console.log(2)
				params = '?returnUrl='+encodeURIComponent(location.href);
			}
			window.location.href = '../member/login.do'+params;
		}else{
			if(c_url != null && c_url != ''){
				window.location.href = c_url;	
			}else{
				window.location.href = '../main/main.do';	
			}
		}
	}
	function showSearchPop(){
		
	    axios.get("../products/listKeywd.json").then((res) => {
	    	search.$children[0].itemHitData = res.data.data;
	    }).catch(err => {
	        console.log(err);
	    });
	
		$('.pop-search-wrap').show();	
		search._data.serachPopShow=true;
		
	}


	
	function emailCheck(c_email){
		var regExp = /^[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_.]?[0-9a-zA-Z])*.[a-zA-Z]{2,3}$/i; 
		if (c_email.match(regExp) != null) { 
			// alert('성공!');
			return false;
		} else { 
			// alert('실패');
			return true;
		}
	}
	
	
	function idCheck(c_id){
		return true;
	}
	
	
	function passwdCheck(c_pw){
		return true;
	}


	function imgListFile(value) {
		  if ( value != null && value != ''  ) {
			  // return resourceUri + value;
			  return value.replace('.','_thumb.');
		  } else{
			  return  '';
		  } 
	}
