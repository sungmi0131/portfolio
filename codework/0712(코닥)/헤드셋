    $('.optionChoice h4').on('click', function (e) {
        e.preventDefault();
        $(this).toggleClass('on');
        if( $(this).next('ul').css('display') != 'none'){
            $(this).next('ul').slideUp();
            return;
        }else{
            $(this).next('ul').slideDown();
            return;
        }
    });
