    function qnaWrite(c_qna_idx) {
        // popUp.popOpen('ajax' ,"./pop-qna.do","pop-type-6")
        $('html, body').addClass('no-scroll');
    	var qnaData = [];
    	if(c_qna_idx != null && c_qna_idx != ''){
    		var param = 'bbs_qna_idx='+c_qna_idx;
    		var url = '../data/view-pord-qna.json';
    		ajaxAsyncFn('post',url,param,'json' ,function(data){
    			qnaData = data
      		})
    	}
    	
        
        var addClass = 'pop-type-6';
        var formHtml = $('#pop_qna_form').html();
        var $body = $('body');
		var popId = "pop" || "pop";
        var popClass = addClass || "";
        var innerClass = 'pop-inner';
        var popHtml;
        if ($('#'+ popId).length) {
            $('#'+ popId).remove();
        }
        
           // scroll.scrollStop();
           $body.append('<div id="'+popId+'" class="'+popClass+'"/>').find('#'+ popId).html('<div class="'+innerClass+'"/>');
           $("#"+popId).find('.'+ innerClass).html(formHtml).end().show();
           
           selectBox();
           
           if(qnaData.state == 'success'){
          	 	$("#pop input[name=bbs_qna_idx]").val(qnaData.data.bbs_qna_idx);
               	$('#pop textarea[name=question_cont]').val(qnaData.data.question_cont);
               	$('#pop input[name=question_title]').val(qnaData.data.question_title);
               	$('#pop input[name=question_type]').val(qnaData.data.question_type);
               	var _question_type_text = '';
               	if(qnaData.data.question_type == 'service'){
               		_question_type_text = '상품서비스';
               	}else if(qnaData.data.question_type == 'defect'){
               		_question_type_text = '상품하자';
               	}else if(qnaData.data.question_type == 'claim'){
               		_question_type_text = '교환/반품';
               	}
               	$('#pop .target-area span.txt').html(_question_type_text);
               	$('#pop input[name=actionType]').val('edit');
          	}
        
    }

