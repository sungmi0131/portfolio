function buyingStore(store_idx){
	
		var url = 'get-store-list.do';
		var param = 'prod_code=WH-1000XM5/LME';
		ajaxFn('post',url,param,'html' ,function(data){
			var formHtml = data;
			var addClass = "pop-type-4";
			var $body = $('body');
			var popId = "pop";
	        var popClass = addClass || "";
	        var innerClass = 'pop-inner';
	        var popHtml;
	        if ($('#'+ popId).length) {
	            $('#'+ popId).remove();
	        }
	        $body.append('<div id="'+popId+'" class="'+popClass+'"/>').find('#'+ popId).html('<div class="'+innerClass+'"/>');
	        $("#"+popId).find('.'+ innerClass)
	        	.html(formHtml)
	        	.end()
	        	.show(); 
	        // window.location.hash = '#storeList';
			 if(window.location.hash != '#storeList'){
		        window.location.hash = '#storeList';
			}	        
	        
			
		})
}

function goStroe(store_idx){
    // history.pushState({ 'page_type': 'storeList', 'store_idx':store_idx }, null, window.location.href);
	window.location.href = '../service/service-store-map.do?store_idx='+store_idx;
}

window.onpageshow = function (event) {
   if (event.persisted || (window.performance && window.performance.navigation.type == 2)) {
       if(window.location.hash == '#storeList'){
      	   buyingStore();
       }
	   
	   /* if(window.history.state.page_type == 'storeList'){
    	   buyingStore();
    	   window.history.state.page_type = '';
       } */
       
	   // console.log('BFCahe로부터 복원됨');
       // window.location.href = '../main/main.do';
       // document.getElementById('_bodyShowView').innerHTML='body > div{display:none}';
    } else {
       // console.log('새로 열린 페이지');
    }
