Vue.component('app-item', {
    // props: ['cates', 'cates2', "bookmark"],
    props: {
        item2: {
            default: []
        },
        j: {},
        localname: {},
        catesitemname: {},
        catesitemname2: {},
        bookmarkToggle: {},
        type: {},
        type2: {},
        cate1:null, cate2:null, cate3:null
    },
    data: () => {
        return {
           // bookmark : this.item2.bookmark,
        }
    },
    created: function () {
      
    },
    filters:{
        comma(val){
            return String(val).replace(/\B(?=(\d{3})+(?!\d))/g, ",");
        }
        
    },
    methods: {
    	isBookmark: function (_ob) {
    		// console.log(_id + ' / ' + isBookmark(_id));
    		
    		return isBookmark(_ob);
        },
        isNew: function (_date) {
        	var _now = new Date();
        	if( _date >=  _now.setDate(_now.getDate() - 7)  ){
        		return true;
        	}else{
        		return false;
        	}
        },
        listBookmark2: function (_item) {
        	listBookmark(_item.prod_idx, this.type , this);
        }
        
    } ,    
    template: `
                <div class="products-item-inner">
                    <div class="products-item-img">
                        <a :href="item2 | prod_url">
                            <img :src="item2.list_image_file | imgListFile" :alt="item2.name"/>
                        </a>

                        <button class="item-bookmark" @click="listBookmark2(item2)" v-if="type == 'scrap'">
                            <img src="../res/img/common/bookmark_icon-on.png" alt="북마크" v-if="isBookmark(item2)">
                            <img src="../res/img/common/bookmark_icon.png" alt="북마크" v-else>
                        </button>                        
                    </div>
                    <div class="products-item-text" v-if="type != 'scrap'">
                        <button class="item-bookmark" @click="listBookmark2(item2)" v-if="type != 's2' && type != 'bottom' && type2 != 'bookHidden'  && type != 'cart' && type != 'quick' && type != 's3'">
                            <img src="../res/img/common/bookmark_icon-on.png" alt="북마크" v-if="isBookmark(item2)">
                            <img src="../res/img/common/bookmark_icon.png" alt="북마크" v-else>
                        </button>
                        <button class="item-bookmark" @click="listBookmark2(item2)" v-if="type == 'quick' ">
                            <img src="../res/img/common/close-icon6.png" alt="북마크" v-if="isBookmark(item2)">
                            <img src="../res/img/common/close-icon6.png" alt="북마크" class="quick-close" v-else>
                        </button>
                        <div class="item-icon-wrap" v-if="type != 's2' && type != 'bottom'  && type != 'cart' && type != 'quick'  && type != 's3'">
                        	<span v-if="this.isNew( item2.reg_dt)" class="icon icon-type-auto1">NEW</span> 
				            <span v-if="item2.best_yn == 'Y'" class="icon icon-type-auto2">B</span>
				            <span v-if="item2.price < item2.origin_price" class="icon icon-type-auto3">할인</span>
				            <span v-if="item2.icon1  > 0 " class="icon icon-type-manual9">EVENT</span>
				            <span v-if="item2.icon2  > 0 " class="icon icon-type-manual1">오픈</span>
				            <span v-if="item2.icon3  > 0 " class="icon icon-type-manual3">GPS</span>
				            <span v-if="item2.icon4  > 0 " class="icon icon-type-manual4">GPS/CELL</span>
				            <span v-if="item2.icon5  > 0 " class="icon icon-type-manual5">리퍼</span>
				            <span v-if="item2.icon6  > 0 " class="icon icon-type-manual6">사전예약</span>
				            <span v-if="item2.icon7  > 0 " class="icon icon-type-manual7">업그레이드</span>
				            <span v-if="item2.icon8  > 0 " class="icon icon-type-manual8">타임딜</span>
				            <span v-if="item2.icon9  > 0 " class="icon icon-type-manual2">진열</span>
				            <span v-if="item2.icon11  > 0 " class="icon icon-type-manual11">쿠폰</span>
				            <span v-if="item2.icon12  > 0 " class="icon icon-type-manual12">무료배송</span>

                        </div>
                        <h4><a class="font-body2 c-sc1" :href="item2 | prod_url">{{item2.name}}</a></h4>
                        <div class="price-wrap">
                            <p class="ori-price lato-regular c-sc2" v-if="type != 's2' && type != 'quick'">
	                            <template v-if="item2.disp_price_yn == 'Y'">
				            		{{item2.disp_price | priceFormat}}
				            	</template>
				            	<template v-else-if="item2.price != item2.origin_price && item2.origin_price != 0 " >
				            		{{item2.origin_price | priceFormat}}
				            	</template>                            
                            </p>
                            <p class="parcents-price lato-regular c-ac2 dp-inb" v-if="type != 'quick' && type != 's2' && item2.price != item2.origin_price && item2.origin_price != 0">
                            	<template v-if="item2.disp_price_yn == 'Y'">
				            		{{100 - Number(item2.price)/Number(item2.disp_price)*100 | dcRate}}%
				            	</template>
				            	<template v-else>
				            		{{100 - Number(item2.price)/Number(item2.origin_price)*100 | dcRate}}%
				            	</template>
                            </p>
                            <p class="now-price lato-bold dp-inb" v-if="type != 'quick'">{{item2.price | priceFormat}}</p>
                        </div>
                        
                        <template  v-if="type != 's2' && type != 'bottom' && type != 'cart' && type != 'quick' && item2.card_view_yn == 'Y' ">
                            <div class="card-sale tosspay-sale"  v-if="item2.card_type == 'T' ">
                                <img src="../res/img/common/tosspay-icon.png" alt="토스페이 아이콘" />
                                <template v-if="item2.card_pg_dc == 0 ">
			                		<template v-if="item2.card_pg_price >= 10000 ">
			                    	{{item2.card_pg_price / 10000}}만원 카드 즉시할인
			                    	</template>
			                    	<template v-else>
			                    	{{item2.card_pg_price / 1000}}천원 카드 즉시할인
			                    	</template>
			                    </template>
			                    <template v-else>
			                    	{{item2.card_rate}}% 카드
			                    </template>                                
                            </div>
                            <div class="card-sale"  v-else >
	                            <img src="../res/img/common/card-icon.png" :alt="item2.card_title"/>
	                            <template v-if="item2.card_pg_dc == 0 ">
			                		<template v-if="item2.card_pg_price >= 10000 ">
			                    	{{item2.card_pg_price / 10000}}만원 카드 즉시할인
			                    	</template>
			                    	<template v-else>
			                    	{{item2.card_pg_price / 1000}}천원 카드 즉시할인
			                    	</template>
			                    </template>
			                    <template v-else>
			                    	{{item2.card_rate}}% 카드
			                    </template>                                
	                        </div>
                        </template>
                       
                    </div>
                    <template  v-if="type == 'cart'">
                        <div class="add-cart-btn fs14 f-wb c-pc1 tc mt20" :onclick="'addCart('+item2.prod_idx+');'">
                            장바구니
                        </div>
                    </template>
                </div>
               
    `,
  
    
})
